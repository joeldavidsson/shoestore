{
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=${POSTGRES_PORT};Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD}"
  },
  "Jwt": {
    "Key": "${JWT__KEY}",
    "Issuer": "${JWT__ISSUER}",
    "Audience": "${JWT__AUDIENCE}"
  },
  "Stripe": {
    "SecretKey": "${STRIPE__SECRET_KEY}",
    "PublishableKey": "${STRIPE__PUBLISHABLE_KEY}"
  },
  "RapidApi": {
    "ApiKey": "${RAPIDAPI__KEY}",
    "Host": "${RAPIDAPI__HOST}",
    "BaseUrl": "${RAPIDAPI__URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
